{
  "info": {
    "name": "Traveloo Backend",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Auth - Register",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "url": {
          "raw": "{{base_url}}/auth/register",
          "host": [
            "{{base_url}}"
          ],
          "path": [
            "auth",
            "register"
          ]
        },
        "body": {
          "mode": "raw",
          "raw": "{\n  \"email\": \"user@example.com\",\n  \"password\": \"password\"\n}"
        }
      }
    },
    {
      "name": "Auth - Login",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "url": {
          "raw": "{{base_url}}/auth/login",
          "host": [
            "{{base_url}}"
          ],
          "path": [
            "auth",
            "login"
          ]
        },
        "body": {
          "mode": "raw",
          "raw": "{\n  \"email\": \"user@example.com\",\n  \"password\": \"password\"\n}"
        }
      }
    },
    {
      "name": "Auth - Me",
      "request": {
        "method": "GET",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer {{token}}"
          }
        ],
        "url": {
          "raw": "{{base_url}}/auth/me",
          "host": [
            "{{base_url}}"
          ],
          "path": [
            "auth",
            "me"
          ]
        }
      }
    },
    {
      "name": "Itin\u00e9raire IA",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "url": {
          "raw": "{{base_url}}/generate-itinerary",
          "host": [
            "{{base_url}}"
          ],
          "path": [
            "generate-itinerary"
          ]
        },
        "body": {
          "mode": "raw",
          "raw": "{\n  \"city\": \"Lisbonne\",\n  \"start_date\": \"2025-08-10\",\n  \"end_date\": \"2025-08-15\",\n  \"profile\": \"solo\",\n  \"preferences\": [\n    \"culture\",\n    \"gastronomie\"\n  ]\n}"
        }
      }
    },
    {
      "name": "Recherche vols",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "url": {
          "raw": "{{base_url}}/api/search",
          "host": [
            "{{base_url}}"
          ],
          "path": [
            "api",
            "search"
          ]
        },
        "body": {
          "mode": "raw",
          "raw": "{\n  \"origin\": \"PAR\",\n  \"destination\": \"LIS\",\n  \"departure_date\": \"2025-08-01\",\n  \"return_date\": \"2025-08-15\",\n  \"class_\": \"Y\",\n  \"adults\": 1,\n  \"children\": 0,\n  \"infants\": 0\n}"
        }
      }
    }
  ],
  "variable": [
    {
      "key": "base_url",
      "value": "https://backend-traveloo.onrender.com"
    },
    {
      "key": "token",
      "value": ""
    }
  ]
